<div class="nb_container">
    <div class="nb_inner_container container-custom">
        <a href="/dashboard">
            <img src="/public/img/alterego/logo.svg" alt="conlus logo">
        </a>
        <div class="nb_links_container">
            <a href="/dashboard" class="nb_link">
                <span>Accueil</span>
            </a>
            <div class="vertical_sep"></div>
            <div class="nb_user_cont">
                <div class="nb_user_img">
                    <svg viewBox="0 0 20 20" fill="currentColor" class="user w-6 h-6">
                        <path fill-rule="evenodd" d="M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z"
                            clip-rule="evenodd"></path>
                    </svg>
                </div>
                <span class="nb_user_name"><%= userName %></span>
            </div>
            <div class="vertical_sep"></div>
            <a href="/logout" class="nb_logout_cont">
                <div class="nb_logout_icon">
                    <svg viewBox="0 0 20 20" fill="currentColor" class="logout w-6 h-6">
                        <path fill-rule="evenodd"
                            d="M3 3a1 1 0 00-1 1v12a1 1 0 102 0V4a1 1 0 00-1-1zm10.293 9.293a1 1 0 001.414 1.414l3-3a1 1 0 000-1.414l-3-3a1 1 0 10-1.414 1.414L14.586 9H7a1 1 0 100 2h7.586l-1.293 1.293z"
                            clip-rule="evenodd"></path>
                    </svg>
                </div>
                <span class="nb_logout_text">Déconnecter</span>
            </a>
        </div>
        <button class="nb_menu_btn" id="nb_menu_open">
            <svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16">
                </path>
            </svg>
        </button>
    </div>
    <!--  -->
    <div id="nb_mobile" class="nb_mobile_container" data-visible='false'>
        <div class="nb_mobile_content">
            <div class="nb_top">
                <a href="/dashboard">
                    <img src="/public/img/alterego/logo_min.svg" alt="conlus logo">
                </a>
                <button class="nb_menu_btn" id="nb_menu_close">
                    <svg class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12">
                        </path>
                    </svg>
                </button>
            </div>
            <div class="nb_content">
                <a href="/dashboard" class="nb_m_link">
                    <svg viewBox="0 0 20 20" fill="currentColor" class="home w-6 h-6">
                        <path
                            d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z">
                        </path>
                    </svg>
                    <span class="nb_m_link_text">Accueil</span>
                </a>
                <div class="horizontal_sep"></div>
                <div class="nb_user_cont">
                    <div class="nb_user_img">
                        <svg viewBox="0 0 20 20" fill="currentColor" class="user w-6 h-6">
                            <path fill-rule="evenodd" d="M10 9a3 3 0 100-6 3 3 0 000 6zm-7 9a7 7 0 1114 0H3z"
                                clip-rule="evenodd"></path>
                        </svg>
                    </div>
                    <span class="nb_user_name"><%= userName %></span>
                </div>
                <div class="horizontal_sep"></div>
                <div class="nb_m_btns_container">
                    <a href="/logout" class="nb_m_link_btn nb_logout">
                        <svg viewBox="0 0 20 20" fill="currentColor" class="logout w-6 h-6">
                            <path fill-rule="evenodd"
                                d="M3 3a1 1 0 00-1 1v12a1 1 0 102 0V4a1 1 0 00-1-1zm10.293 9.293a1 1 0 001.414 1.414l3-3a1 1 0 000-1.414l-3-3a1 1 0 10-1.414 1.414L14.586 9H7a1 1 0 100 2h7.586l-1.293 1.293z"
                                clip-rule="evenodd"></path>
                        </svg>
                        <span class="nb_logout_text">Déconnecter</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>
<!--  -->
<link rel="stylesheet" href="/public/css/alterego/navbar.css" />
<script src="/public/js/alterEgo/_navBar.js"></script>