<!-- MAIN AREA -->
<div>
    <!-- INCOMING NOTIFS -->
    <!-- connections -->
    <!-- <div id="clientConsul"> -->
    <% if(!extra.clients){ %>
    <div id="activeConsul">
        <h4>Consultations active :</h4>
        <%- include('./partials/empty') %>
    </div>
    <% }else{ %>
    <% if(extra.clients.length <= 0){ %>
    <div id="historyConsul">
        <h4>Hisotrique de consultations :</h4>
        <%- include('./partials/empty') %>
    </div>
    <% }else{ %>
    <div id="activeConsul">
        <h4>Consultations active :</h4>
        <% for(var i=0;i < extra.clients.length;i++){ %>
        <% if(extra.clients[i].consulState == -1){ %>
        <%- include('./partials/consultation_card', {consultation : extra.clients[i]}) %>
        <% }%>
        <% } %>
    </div>
    <div id="historyConsul">
        <h4>Hisotrique de consultations :</h4>
        <h4>Consultations active :</h4>
        <% for(var i=0;i < extra.clients.length;i++){ %>
        <% if(extra.clients[i].consulState != -1){ %>
        <%- include('./partials/history', {consultation : extra.clients[i]}) %>
        <% }%>
        <% } %>
    </div>
    <% } %>
    <% } %>
    <!-- </div> -->
</div>
